  <div class="modal fade " id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Connect with Us</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true" style="color: #fff;">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="row row-content">

            <div class="col-12">
              <form method="post" action="/contact" enctype=”multipart/form-data”>
                <div class="form-group row">
                  <label for="name" class="col-12 col-md-3 ml-2 col-form-label"> Name</label>
                  <div class="col-md-8 mr-1">
                    <input type="text" class="form-control" name="name" id="name" placeholder="Name">
                  </div>
                </div>
                <div class="form-group row">
                  <label for="telnum" class="col-12 col-md-3 ml-2 col-form-label">Contact No.</label>
                  <div class="col-md-8 mr-1">
                    <input type="tel" class="form-control " id="telnum" name="telnum" placeholder="Tel. number">
                  </div>
                </div>
                <div class="form-group row">
                  <label for="emailid" class="col-12 col-md-3 ml-2 col-form-label">Email</label>
                  <div class="col-md-8 mr-1">
                    <input type="email" class="form-control" id="emailid" name="emailid" placeholder="Email">
                  </div>
                </div>

                <div class="form-group row">
                  <label for="message" class="col-12 ml-2 col-form-label">Your Message</label>
                  <div class="col-md-11 ml-2">
                    <textarea class="form-control" id="message" name="message" rows="6"></textarea>
                  </div>
                </div>
                <div class="form-group row">
                  <div class="ml-2 col-md-10">
                    <input type="submit" class="btn btn-dark" value="Connect">
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>

      </div>
    </div>
